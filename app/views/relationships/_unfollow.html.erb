<% if current_user.relationships.find_by_followed_id(@user.id).nil? %>
  <%= puts @user.id %>
<% else %>
    <%= form_for current_user.relationships.find_by_followed_id(@user.id), :remote => true,
                 :html => { :method => :delete } do |f| %>
        <div class="actions"><%= f.submit "Unfollow" %></div>
    <% end %>
<% end %>
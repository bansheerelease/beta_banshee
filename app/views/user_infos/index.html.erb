<h1>Listing user_infos</h1>

<table>
  <tr>
    <th>First name</th>
    <th>Last name</th>
    <th>Home address</th>
    <th>Work address</th>
    <th>Skype</th>
    <th>Birthday</th>
    <th>Family status</th>
    <th>Home city</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @user_infos.each do |user_info| %>
  <tr>
    <td><%= user_info.first_name %></td>
    <td><%= user_info.last_name %></td>
    <td><%= user_info.home_address %></td>
    <td><%= user_info.work_address %></td>
    <td><%= user_info.skype %></td>
    <td><%= user_info.birthday %></td>
    <td><%= user_info.family_status %></td>
    <td><%= user_info.home_city %></td>
    <td><%= link_to 'Show', user_info %></td>
    <td><%= link_to 'Edit', edit_user_info_path(user_info) %></td>
    <td><%= link_to 'Destroy', user_info, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New User info', new_user_info_path %>

<table>
  <% if !@users.empty? %>
      <% @users.each do |user| %>
        <tr>
            <td> <%= user.email %> </td>
            <td>
              <% @user = user %>
              <% if !@user.nil? %>
                <%= render 'relationships/unfollow' %>
              <% end %>
            </td>
            <td> <%= link_to 'Show him','/wall/' + user.id.to_s, :class => "button round" %></td>
        </tr>
      <% end %>
      <%= paginate @users %>
  <% end %>
</table>